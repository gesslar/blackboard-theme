{
  "name": "blackboard-themepack",
  "displayName": "Blackboard Theme Pack",
  "description": "Classic blackboard aesthetics for night coding - chalk on slate with electric blue accents",
  "version": "0.0.3",
  "publisher": "gesslar",
  "license": "Unlicense",
  "engines": {
    "vscode": ">=1.50.0"
  },
  "scripts": {
    "lint": "npx @gesslar/aunty@latest lint ./src/themes/blackboard.yaml",
    "watch": "npx @gesslar/aunty@latest build -wo ./dist ./src/themes/*.yaml",
    "watch-nerd": "npx @gesslar/aunty@latest build --nerd -wo ./dist ./src/themes/*.yaml",
    "build": "npx @gesslar/aunty@latest build -o ./dist ./src/themes/*.yaml",
    "package": "npm run build && vsce package --skip-license",
    "submit": "npm run package && xdg-open https://marketplace.visualstudio.com/manage/publishers/gesslar"
  },
  "categories": [
    "Themes"
  ],
  "type": "module",
  "contributes": {
    "themes": [
      {
        "label": "Blackboard",
        "description": "Don't use your nails, eek!",
        "id": "blackboard",
        "uiTheme": "vs-dark",
        "path": "dist/blackboard.color-theme.json"
      },
      {
        "label": "Bubblegum Goth",
        "description": "*POP* goes the emo",
        "id": "bubblegum-goth",
        "uiTheme": "vs-dark",
        "path": "dist/bubblegum-goth.color-theme.json"
      },
      {
        "label": "Corporate",
        "description": "They'll definitely think you're working now.",
        "id": "corporate",
        "uiTheme": "vs-dark",
        "path": "dist/corporate.color-theme.json"
      }
    ]
  },
  "icon": "media/blackboard.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gesslar/blackboard-theme"
  },
  "keywords": [
    "theme",
    "blackboard",
    "dark",
    "chalk",
    "night",
    "blue"
  ],
  "__metadata": {
    "id": "blackboard-theme"
  }
}
