      "scope": "keyword.operator.new, keyword.operator.expression, keyword.operator.word.new, keyword.operator.namespace, keyword.operator.borrow.and, keyword.operator.dereference, keyword.operator.key-value, keyword.operator.access.dot, keyword.operator.bitwise, keyword.operator.address",
      "settings": {
        "foreground": "#9c86b3"
      }
    },
    {
      "name": "Arithmetic Operators",
      "scope": "keyword.operator.arithmetic",
      "settings": {
        "foreground": "#9c86b3"
      }
    },
    {
      "name": "Assignment Operator",
      "scope": "keyword.operator.assignment, punctuation.separator.key-value",
      "settings": {
        "foreground": "#9c86b3"
      }
    },
    {
      "name": "Logical Operators",
      "scope": "keyword.operator.logical, keyword.operator.or.regexp, keyword.operator.negation.regexp",
      "settings": {
        "foreground": "#9c86b3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "General Keyword Operators",
      "scope": "keyword.operator, operator, keyword.operator.gradient, keyword.operator.quantifier.regexp, keyword.operator.key",
      "settings": {
        "foreground": "#9c86b3"
      }
    },
    {
      "name": "Universal Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#6d7f89cc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Universal Self References",
      "scope": "variable.language.self, variable.language.this, variable.language.base, variable.language.super",
      "settings": {
        "foreground": "#c7856d"
      }
    },
    {
      "name": "Universal Booleans",
      "scope": "constant.language.boolean, constant.language.null",
      "settings": {
        "foreground": "#7ba66d"
      }
    },
    {
      "name": "Universal String Interpolation",
      "scope": "punctuation.definition.template-expression, string.interpolated.expression, punctuation.definition.variable, punctuation.section.embedded.begin, punctuation.section.embedded.end",
      "settings": {
        "foreground": "#ba95b4"
      }
    },
    {
      "name": "Universal Punctuation",
      "scope": "punctuation, begin.brace, end.brace, punctuation.terminator.expression, punctuation.separator.inheritance",
      "settings": {
        "foreground": "#809cc2"
      }
    },
    {
      "name": "Semantic Types Fallback",
      "scope": "namespace, class, enum, interface, struct, typeParameter, type",
      "settings": {
        "foreground": "#80a674"
      }
    },
    {
      "name": "Semantic Callables Fallback",
      "scope": "function, method, macro",
      "settings": {
        "foreground": "#bd9551"
      }
    },
    {
      "name": "Semantic Identifiers Fallback",
      "scope": "parameter, variable, property, enumMember, decorator, event",
      "settings": {
        "foreground": "#88a1c2"
      }
    },
    {
      "name": "Semantic Literals Fallback",
      "scope": "number, regexp",
      "settings": {
        "foreground": "#809cc2"
      }
    },
    {
      "name": "Semantic Keywords Fallback",
      "scope": "keyword.declaration, label",
      "settings": {
        "foreground": "#ba95b4"
      }
    },
    {
      "name": "Final Fallback - Base Source",
      "scope": "source",
      "settings": {
        "foreground": "#f2f0da"
      }
    },
    {
      "name": "Final Fallback - Generic Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ba95b4"
      }
    },
    {
      "name": "Final Fallback - Generic Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#88a1c2"
      }
    },
    {
      "name": "Final Fallback - Generic Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#80a674"
      }
    }
  ]
}
